<!DOCTYPE html>
<html>
    <head>
        <meta charset="utf-8">
        <title>W3D6 Q3Q4</title>
        <link rel="stylesheet" href="css/style.css" type="text/css">
    </head>
<body>
<%- include('header'); -%>

<div class="contents">
<h2>Shopping Cart Items </h2>
    <% for(let prod of products) { %>
        <div class="productitem">
            <p>Id: <%= prod.id %></p>
            <p>Name: <%= prod.name %></p>
            <p>Price: <%= prod.price %></p>
            <p>Quantity: <%= prod.units %></p>
        </div>
    <% } %>
        
</div>

<%- include('footer'); -%>
</body>
</html>